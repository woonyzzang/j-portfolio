					<!-- portfolio -->
					<!-- [D] swipe 기능 활성화시 v1 클래스 추가/제거 -->
					<article id="{{menu.portfolio.name}}" class="{{menu.portfolio.name}}">
						<h2>{{menu.portfolio.name}}</h2>
						<div class="inr">
							<nav>
								<h3>Filter Menu</h3>
								<!-- [D] 활성화 확장 클래스 .active 추가/제거 -->
