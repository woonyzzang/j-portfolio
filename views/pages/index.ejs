								<td class="role">{{workbench.role}}</td>
								<td class="work">
                                    <span class="label_area">
                                        <i title="가이드" class="guide" data-ng-if="workbench.works.guide">G</i>
                                        <i title="마크업" class="markup" data-ng-if="workbench.works.markup">M</i>
                                        <i title="UI개발" class="uidev" data-ng-if="workbench.works.uidev">UI</i>
                                        <i title="프론트엔드개발" class="frontdev" data-ng-if="workbench.works.frontdev">FE</i>
                                        <i title="백엔드개발" class="backdev" data-ng-if="workbench.works.backdev">BE</i>
                                    </span>
								</td>
								<td class="work">
								    <span data-ng-repeat="skill in workbench.skills">{{($first)? '' : ', '}}{{skill}}</span>
